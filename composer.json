{
    "name": "andreaz/search-table",
    "description": "Easy to use models table",
    "keywords": [
        "Andrea Zorzi",
        "laravel",
        "laravel-search-table",
        "search-table"
    ],
    "homepage": "https://github.com/andreaz/search-table",
    "autoload": {
        "psr-4": {
            "SearchTable\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Andrea Zorzi",
            "email": "info@zorziandrea.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/support": "11.x-dev",
        "illuminate/http": "11.x-dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SearchTable\\SearchTableServiceProvider"
            ],
            "aliases": {
                "SearchTable": "SearchTable\\Facades\\SearchTable"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
